{
    "name": "silarhi/caf-parser",
    "description": "A CAF Parser for LA44ZZ file",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "caf",
        "parser",
        "LA44ZZ"
    ],
    "authors": [
        {
            "name": "SILARHI",
            "email": "hello@silarhi.fr"
        }
    ],
    "require": {
        "php": ">= 7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.49",
        "phpunit/phpunit": "^8.5.21|^9.5.10",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Silarhi\\Caf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silarhi\\Caf\\Tests\\": "tests/"
        }
    }
}
